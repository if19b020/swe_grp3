<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PresentationLayer.controller.TourViewController">
    <HBox layoutY="2.0" prefHeight="8.0" prefWidth="600.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="5.0">
        <Label prefHeight="24.0" prefWidth="100.0" text="Tour Name" HBox.hgrow="NEVER">
            <HBox.margin>
                <Insets right="5.0" />
            </HBox.margin>
            <padding>
                <Insets left="14.0" />
            </padding>
        </Label>
        <TextField fx:id="nameField" prefHeight="25.0" prefWidth="523.0" HBox.hgrow="ALWAYS" />
    </HBox>
    <HBox layoutY="27.0" prefHeight="8.0" prefWidth="600.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="35.0">
        <Label prefHeight="24.0" prefWidth="100.0" text="Start Point" HBox.hgrow="NEVER">
            <HBox.margin>
                <Insets right="5.0" />
            </HBox.margin>
            <padding>
                <Insets left="14.0" />
            </padding>
        </Label>
        <TextField fx:id="startField" prefHeight="25.0" prefWidth="523.0" HBox.hgrow="ALWAYS" />
    </HBox>
    <HBox layoutY="52.0" prefHeight="8.0" prefWidth="600.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="65.0">
        <Label prefHeight="24.0" prefWidth="100.0" text="End Point" HBox.hgrow="NEVER">
            <HBox.margin>
                <Insets right="5.0" />
            </HBox.margin>
            <padding>
                <Insets left="14.0" />
            </padding>
        </Label>
        <TextField fx:id="endField" prefHeight="25.0" prefWidth="523.0" HBox.hgrow="ALWAYS" />
    </HBox>
    <HBox layoutY="101.0" prefHeight="211.0" prefWidth="600.0" AnchorPane.rightAnchor="14.0">
        <Label prefHeight="24.0" prefWidth="96.0" text="Description" HBox.hgrow="NEVER">
            <HBox.margin>
                <Insets right="5.0" />
            </HBox.margin>
        </Label>
        <TextArea fx:id="descriptionField" prefHeight="211.0" prefWidth="530.0" HBox.hgrow="ALWAYS" />
    </HBox>
    <Button fx:id="confirmButton" layoutX="530.0" layoutY="361.0" mnemonicParsing="false" onAction="#addAction" prefHeight="25.0" prefWidth="86.0" text="Confirm" AnchorPane.bottomAnchor="14.0" AnchorPane.rightAnchor="14.0" />
</AnchorPane>
